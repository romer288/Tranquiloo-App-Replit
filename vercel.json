{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api?path=:path"
    },
    {
      "source": "/auth/:path*",
      "destination": "/api?path=auth/:path*"
    }
  ],
  "outputDirectory": "dist/public",
  "env": {
    "NODE_ENV": "production"
  }
}
