{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/dist"
    },
    {
      "source": "/(.*)",
      "destination": "/api/dist"
    }
  ],
  "outputDirectory": "dist/public",
  "env": {
    "NODE_ENV": "production"
  }
}
